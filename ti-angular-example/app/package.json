{
    "name": "titanium-angular-dev-app",
    "version": "1.0.0",
    "description": "Test app for the Titanium Angular platform",
    "main": "dist/index.js",
    "scripts": {
        "test": "echo 'No tests specified'",
        "tsc": "tsc -p tsconfig.json",
        "ngc": "ngc -p tsconfig.json",
        "build": "npm run tsc && rollup -c rollup.config.js && cp renderer-test.html ../Resources/",
        "tsc-tia": "tsc -p ../../packages/titanium-angular/tsconfig.json",
        "dev": "npm run tsc-tia && npm run build && cd .. && appc run -p ios -l trace",
        "ios": "npm run build && cd .. && appc run -p ios",
        "android": "npm run build && cd .. && appc run -p android"
    },
    "author": "Jan Vennemann",
    "license": "Apache-2.0",
    "repository": "https://github.com/appcelerator/titanium-angular",
    "dependencies": {
        "@angular/common": "^5.0.0",
        "@angular/compiler": "^5.0.0",
        "@angular/compiler-cli": "^5.0.0",
        "@angular/core": "^5.0.0",
        "@angular/platform-browser": "^5.0.0",
        "@angular/platform-browser-dynamic": "^5.0.0",
        "rxjs": "^5.5.0",
        "titanium-angular": "file:../../packages/titanium-angular",
        "typescript": "^2.5.3",
        "zone.js": "^0.8.18"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "rollup": "^0.50.0",
        "rollup-plugin-alias": "^1.4.0",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-commonjs": "^8.2.4",
        "rollup-plugin-inject": "^2.0.0",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-node-resolve": "^3.0.0"
    }
}
